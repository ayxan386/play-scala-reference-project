# Routes

GET        /api/v1/users            controllers.UserController.getById(id: Long)

GET        /api/v1/users/all        controllers.UserController.getAll

POST        /api/v1/users/add       controllers.UserController.addUser

->          /api/v1/posts           routers.PostRoutes

->          /api/v1/comments        routers.CommentRoutes

POST        /api/v1/images/db			controllers.ImageController.saveImageDB
POST        /api/v1/images/direct			controllers.ImageController.saveImageDirect

GET 				/api/v1/images/direct					controllers.ImageController.getByName(name: String)
GET 				/api/v1/images/db					controllers.ImageController.getByNameFromDB(name: String)


      